{
  "name": "strath-demo",
  "version": "1.0.0",
  "description": "Sandbox",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "xbuild": "tsc -p src/",
    "xbuild:watch": "tsc -p src/ -w",
    "xbuild:e2e": "tsc -p e2e/",
    "xserve": "lite-server -c=bs-config.json",
    "xserve:e2e": "lite-server -c=bs-config.e2e.json",
    "xprestart": "npm run build",
    "xstart": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "xpree2e": "npm run build:e2e",
    "xe2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "xpreprotractor": "webdriver-manager update",
    "xprotractor": "protractor protractor.config.js",
    "xpretest": "npm run build",
    "xtest": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "xpretest:once": "npm run build",
    "xtest:once": "karma start karma.conf.js --single-run",
    "xlint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~4.2.6",
    "@angular/common": "~4.2.6",
    "@angular/compiler": "~4.2.6",
    "@angular/core": "~4.2.6",
    "@angular/forms": "~4.2.6",
    "@angular/http": "~4.2.6",
    "@angular/platform-browser": "~4.2.6",
    "@angular/platform-browser-dynamic": "~4.2.6",
    "@angular/router": "~4.2.6",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.26",
    "angular-calendar": "0.17.0",
    "angular-draggable-droppable": "^1.0.1",
    "angular-in-memory-web-api": "^0.3.2",
    "angular-resizable-element": "^1.1.1",
    "angular2-adal": "^0.1.4",
    "calendar-utils": "0.0.56",
    "core-js": "^2.4.1",
    "date-fns": "^1.28.5",
    "font-awesome": "^4.7.0",
    "ng2-adal": "^1.0.1",
    "rrule": "^2.2.0",
    "rxjs": "^5.0.1",
    "typings": "^2.1.1",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.12",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.4",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "jasmine-core": "~2.6.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "protractor": "~5.1.2",
    "rimraf": "^2.5.4",
    "tslint": "^5.3.2",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.18.2",
    "typescript": "^2.1.0",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.1.0"
  },
  "repository": {}
}
